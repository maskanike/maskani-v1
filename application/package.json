{
  "name": "maskani",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "unseed": "./node_modules/.bin/sequelize db:seed:undo:all",
    "seed": "yarn unseed && ./node_modules/.bin/sequelize db:seed:all",
    "start": "yarn migrate && nodemon --exec babel-node ./app.js",
    "test": "export NODE_ENV=test && ./node_modules/.bin/sequelize db:migrate:undo:all && yarn migrate && mocha --require @babel/register 'tests/**/**test.js' --exit"
  },
  "dependencies": {
    "argon2": "^0.24.1",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-jwt": "^5.3.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "pg": "^7.12.1",
    "pug": "^2.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.6.2",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.1"
  }
}
